{
  "expo": {
    "scheme": "treat",
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to find nearby coffee spots."
        }
      ]
    ],
    "name": "treat",
    "slug": "treat",
    "icon": "./assets/images/icon.png",
    "ios": {
      "icon": "./assets/images/ios/icon-1024.png",
      "bundleIdentifier": "com.treatranked.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8A3DFF"
      },
      "package": "com.treat.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dfdeaa9e-e4c2-4303-b70b-72edbad26155"
      }
    }
  }
}
